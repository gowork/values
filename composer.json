{
    "name": "gowork/values",
    "type": "library",
    "description": "PHP primitive types value objects.",
    "keywords": ["primitive", "type", "value object"],
    "license": "MIT",
    "authors": [
        {
            "name": "Damian Zaprzałek",
            "email": "sz.p.damian@gmail.com"
        },
        {
            "name": "Bronisław Białek",
            "email": "after89@gmail.com"
        },
        {
            "name": "Dawid Łakomski",
            "email": "dawid.lakomski@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpspec/phpspec": "^6.1 || ^7.0",
        "phpstan/phpstan": "^0.12",
        "phpbench/phpbench": "@dev",
        "phpunit/phpunit": "^8.5 || ^9.0",
        "phpstan/extension-installer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "GW\\Value\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\GW\\Value\\": "spec/",
            "tests\\GW\\Value\\": "tests/",
            "doc\\GW\\Value\\": "examples/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    }
}
